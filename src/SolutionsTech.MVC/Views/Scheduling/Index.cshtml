@model IEnumerable<SolutionsTech.MVC.Models.Scheduling>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DtCreate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TotalValue)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Observation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.User)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FormPayment)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DtCreate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalValue)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Observation)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.User.IdUser)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FormPayment.IdFormPayment)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdScheduling">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdScheduling">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdScheduling">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
